{
  "name": "Tetris HA",
  "version": "1.0.2",
  "slug": "tetris_ha",
  "description": "Tetris game visualizer for LED curtain via BLE",
  "startup": "application",
  "boot": "auto",
  "arch": ["aarch64", "armv7", "amd64"],
  "host_network": true,
  "full_access": true,
  "devices": [
    "/dev/snd",
    "/dev/bus/usb",
    "/dev/ttyAMA0",
    "/dev/ttyS0",
    "/var/run/dbus/system_bus_socket:/var/run/dbus/system_bus_socket"
  ],
  "privileged": ["SYS_RAWIO", "SYS_ADMIN", "NET_ADMIN", "DAC_READ_SEARCH"],
  "options": {},
  "schema": {},
  "build": {
    "args": {
      "BUILD_FROM": "ghcr.io/hassio-addons/base:14.4.1"
    }
  }
}
